<?xml version="1.0" encoding="UTF-8"?>
<project name="module_p140_grades_student" default="compile.module.p140_grades_student">
  <dirname property="module.p140_grades_student.basedir" file="${ant.file.module_p140_grades_student}"/>
  
  <property name="module.jdk.home.p140_grades_student" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.p140_grades_student" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.p140_grades_student" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.p140_grades_student" value="-encoding UTF-8 -source 11 -target 11 ${compiler.args}"/>
  
  <property name="p140_grades_student.output.dir" value="${module.p140_grades_student.basedir}/out/production/P140_grades_student"/>
  <property name="p140_grades_student.testoutput.dir" value="${module.p140_grades_student.basedir}/out/test/P140_grades_student"/>
  
  <path id="p140_grades_student.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="p140_grades_student.module.production.classpath">
    <path refid="${module.jdk.classpath.p140_grades_student}"/>
  </path>
  
  <path id="p140_grades_student.runtime.production.module.classpath">
    <pathelement location="${p140_grades_student.output.dir}"/>
  </path>
  
  <path id="p140_grades_student.module.classpath">
    <path refid="${module.jdk.classpath.p140_grades_student}"/>
    <pathelement location="${p140_grades_student.output.dir}"/>
  </path>
  
  <path id="p140_grades_student.runtime.module.classpath">
    <pathelement location="${p140_grades_student.testoutput.dir}"/>
    <pathelement location="${p140_grades_student.output.dir}"/>
  </path>
  
  
  <patternset id="excluded.from.module.p140_grades_student">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.p140_grades_student">
    <patternset refid="excluded.from.module.p140_grades_student"/>
  </patternset>
  
  <path id="p140_grades_student.module.sourcepath">
    <dirset dir="${module.p140_grades_student.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.p140_grades_student" depends="compile.module.p140_grades_student.production,compile.module.p140_grades_student.tests" description="Compile module P140_grades_student"/>
  
  <target name="compile.module.p140_grades_student.production" depends="register.custom.compilers" description="Compile module P140_grades_student; production classes">
    <mkdir dir="${p140_grades_student.output.dir}"/>
    <javac2 destdir="${p140_grades_student.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.p140_grades_student}/javac">
      <compilerarg line="${compiler.args.p140_grades_student}"/>
      <bootclasspath refid="p140_grades_student.module.bootclasspath"/>
      <classpath refid="p140_grades_student.module.production.classpath"/>
      <src refid="p140_grades_student.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.p140_grades_student"/>
    </javac2>
    
    <copy todir="${p140_grades_student.output.dir}">
      <fileset dir="${module.p140_grades_student.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.p140_grades_student.tests" depends="register.custom.compilers,compile.module.p140_grades_student.production" description="compile module P140_grades_student; test classes" unless="skip.tests"/>
  
  <target name="clean.module.p140_grades_student" description="cleanup module">
    <delete dir="${p140_grades_student.output.dir}"/>
    <delete dir="${p140_grades_student.testoutput.dir}"/>
  </target>
</project>